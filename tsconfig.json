{
	"compilerOptions": {
		"target": "es2020",
		"module": "ES2020",
		"moduleResolution": "Node",
		"lib": ["es2020"],
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"noImplicitAny": true,
		"strict": true,
		"esModuleInterop": true,
		"importsNotUsedAsValues": "error",
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./",
		"outDir": "build",
		"sourceMap": true,
		"paths": {
			"types": ["../../types.d.ts"]
		}
	}
}
